<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title – TripHelper</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/Styles.css")
    <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>



</head>
<body style="background: white">
    <div class="navbar navbar-inverse navbar-fixed-top">
        
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("TripHelper", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Построить маршрут", "ConstructTrack", "Home")</li>
                    <li>@Html.ActionLink("Поиск", "Search", "Track")</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        
    </div>
<div class="body-content">
    @RenderBody()
    <hr />
    <footer>
        <p>&copy; @DateTime.Now.Year – приложение TripHelper</p>
    </footer>
</div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/bundles/modernizr")
@Styles.Render("~/Content/jquery.formstyler.css")
@Styles.Render("~/Content/material.css")
@Scripts.Render("~/Scripts/jquery.unobtrusive-ajax.min.js")
<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
<script src="http://malsup.github.com/jquery.form.js"></script>
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
<link rel="stylesheet" href="~/Content/jquery-bar-rating-master/dist/themes/fontawesome-stars-o.css">
    <link rel="stylesheet" href="~/Content/jquery-bar-rating-master/dist/themes/fontawesome-stars.css">
<script src="~/Content/jquery-bar-rating-master/dist/jquery.barrating.min.js"></script>

    @RenderSection("scripts", required: false)

</body>
</html>
